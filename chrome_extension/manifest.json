{
"name": "Getting Started Example",
"description": "Build an Extension!",
"version": "1.0",
"manifest_version": 3,
"background":{
"service_worker": "background.js"
},
"permissions":[
    "storage",
    "scripting",
    "webNavigation",
    "activeTab"
],
"host_permissions":[
    "*://*/*"
],
"action":{
"default_popup": "popup.html"
}
}
