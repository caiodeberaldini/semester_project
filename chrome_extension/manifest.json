{
"name": "Getting Started Example",
"description": "Build an Extension!",
"version": "1.0",
"manifest_version": 3,
"background":{
"service_worker": "background.js"
},
"permissions":[
    "storage",
    "scripting",
    "webNavigation",
    "activeTab",
    "tabs"
],
"host_permissions":[
    "*://*/*"
],
"action":{
"default_popup": "popup.html"
},
"content_scripts":[
    {
        "matches": ["https://github.com/*"],
        "css": ["content.css"],
        "js": ["content.js"]
    }
],
"web_accessible_resources": [
    {
      "resources": ["template.html"],
      "matches": ["https://github.com/*"]
    }
  ]
}
